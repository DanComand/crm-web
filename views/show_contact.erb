<h1><%= @contact.first_name %> <%= @contact.last_name %></h1>
<p><strong>Email:</strong> <%= @contact.email %></p>
<p><strong>Note: </strong><%= @contact.note %></p>
<div class="button">
	<div class="contacts-new-text">
		<a href="/edit_contact/<%= @contact.id%>">Edit contact</a>
	</div>
</div>


<div class="button">
	<div class="contacts-new-text">
		<a href="/contacts/<%= @contact.id%>/delete">Delete contact</a>
	</div>
</div>

